{
  "name": "gulp-template",
  "version": "0.0.0",
  "description": "Project template for gulp.js",
  "main": "gulpfile.js",
  "scripts": {
    "prepublish": "bower install",
    "start": "gulp",
    "build": "gulp build"
  },
  "keywords": [
